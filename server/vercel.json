{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "server.js"
      }
    ]
    
}